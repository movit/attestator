<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:row="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:menu="urn:import:com.sencha.gxt.widget.core.client.menu"
	xmlns:button="urn:import:com.sencha.gxt.widget.core.client.button"
	xmlns:form="urn:import:com.sencha.gxt.widget.core.client.form"
	xmlns:toolbar="urn:import:com.sencha.gxt.widget.core.client.toolbar"
	xmlns:grid="urn:import:com.sencha.gxt.widget.core.client.grid"
	xmlns:att="urn:import:com.attestator.admin.client.ui.widgets">

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="outerMargins">
		<ui:attributes top="20" right="20" bottom="20" left="20" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="northMargins">
		<ui:attributes top="5" right="5" bottom="5" left="5" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="westMargins">
		<ui:attributes top="0" right="5" bottom="0" left="0" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="centerMargins">
		<ui:attributes top="0" right="0" bottom="0" left="0" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="eastMargins">
		<ui:attributes top="0" right="5" bottom="0" left="5" />
	</ui:with>
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="southMargins">
		<ui:attributes top="5" right="0" bottom="5" left="0" />
	</ui:with>

	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="outerData">
		<ui:attributes margins="{outerMargins}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="northData">
		<ui:attributes size="100" margins="{northMargins}"
			collapsible="true" split="true" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="westData">
		<ui:attributes size="492" margins="{westMargins}"
			collapsible="true" split="true" maxSize="1000" />
	</ui:with>
	<ui:with type="com.sencha.gxt.widget.core.client.container.MarginData"
		field="centerData">
		<ui:attributes margins="{centerMargins}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="eastData">
		<ui:attributes size="150" margins="{eastMargins}"
			collapsible="true" split="true" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BorderLayoutContainer.BorderLayoutData"
		field="southData">
		<ui:attributes size="100" margins="{southMargins}"
			collapsible="true" split="true" maxSize="1000" />
	</ui:with>

	<ui:with type="com.attestator.common.client.ui.resolurces.Resources.Icons"
		field="icons" />

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="bottomMargin">
		<ui:attributes top="0" right="0" bottom="10" left="0" />
	</ui:with>

	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="leftMargin">
		<ui:attributes top="0" right="0" bottom="0" left="10" />
	</ui:with>

	<ui:with field="maxWidthMinHeightBottomMarginVLData"
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="-1" margins="{bottomMargin}" />
	</ui:with>

	<ui:with field="maxWidthMinHeightVLData"
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="-1" />
	</ui:with>

	<ui:with field="minWidthMinHeightVLData"
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="-1" height="-1" />
	</ui:with>

	<ui:with field="maxWidthMaxHeightVLData"
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="1" />
	</ui:with>

	<ui:with field="maxWidthHalfHeightVLData"
		type="com.sencha.gxt.widget.core.client.container.VerticalLayoutContainer.VerticalLayoutData">
		<ui:attributes width="1" height="0.5" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="maxWidthMaxHeightHLData">
		<ui:attributes width="1" height="1" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="minWidthMaxHeightHLData">
		<ui:attributes width="-1" height="1" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="halfWidthMaxHeightHLData">
		<ui:attributes width="0.5" height="1" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.HorizontalLayoutContainer.HorizontalLayoutData"
		field="halfWidthMaxHeightLeftMarginHLData">
		<ui:attributes width="0.5" height="1" margins="{leftMargin}" />
	</ui:with>

	<ui:with type="com.sencha.gxt.widget.core.client.button.Tools.ToolStyle"
		field="toolIcons" />

	<ui:import field="com.sencha.gxt.widget.core.client.button.ToolButton.*" />

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.AccordionLayoutContainer.AccordionLayoutAppearance"
		field="accordionLayoutAppearance" />
	<ui:with
		type="com.sencha.gxt.widget.core.client.ContentPanel.ContentPanelAppearance"
		field="contentPanelAppearance" />
	
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="margins0">
		<ui:attributes top="0" right="0" bottom="0" left="0" />
	</ui:with>
	
	<ui:with type="com.sencha.gxt.core.client.util.Margins" field="margins0050">
		<ui:attributes top="0" right="0" bottom="5" left="0" />
	</ui:with>

	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData"
		field="boxLayoutDataMargins0">
		<ui:attributes margins="{margins0}" />
	</ui:with>
	<ui:with
		type="com.sencha.gxt.widget.core.client.container.BoxLayoutContainer.BoxLayoutData"
		field="boxLayoutDataMargins0050">
		<ui:attributes margins="{margins0050}" />
	</ui:with>
	<gxt:Window ui:field="window" pixelSize="1000, 600" modal="true"
		maximizable="true" resizable="true" blinkModal="true" headingText="Тест"
		focusWidget="{cancelButton}">
		<row:VerticalLayoutContainer ui:field="top">
			<row:child layoutData="{maxWidthMinHeightBottomMarginVLData}">
				<form:FieldLabel labelAlign="TOP" text="Название">
					<form:widget>
						<form:TextField ui:field="name" width="1"
							allowBlank="true" />
					</form:widget>
				</form:FieldLabel>
			</row:child>
			<row:child layoutData="{maxWidthMaxHeightVLData}">
				<row:BorderLayoutContainer borders="false">
					<row:west layoutData="{westData}">
						<gxt:ContentPanel appearance="{contentPanelAppearance}"
							headingText="Элементы которые можно добавить в тест">
							<row:AccordionLayoutContainer ui:field="elementsSourceLayout" borders="false"
								expandMode="SINGLE_FILL">

								<gxt:ContentPanel ui:field="groupsPanel" appearance="{accordionLayoutAppearance}" borders="false"
									headingText="Группы" animCollapse="false">
									<row:VerticalLayoutContainer>
										<row:child layoutData="{minWidthMinHeightVLData}">
											<toolbar:ToolBar visible="true">
												<button:TextButton ui:field="addGroupButton"
													icon="{icons.next16x16}" iconAlign="RIGHT" text="Добавить в тест" />
												<toolbar:FillToolItem />
												<att:SearchField ui:field="groupsSearchField"
													validationDelay="700" emptyText="Поиск" />
											</toolbar:ToolBar>
										</row:child>
										<row:child layoutData="{maxWidthMaxHeightVLData}">
											<grid:Grid ui:field="groupsGrid" />
										</row:child>
										<row:child layoutData="{maxWidthMinHeightVLData}">
											<toolbar:PagingToolBar ui:field="groupsPager"
												pageSize="50" reuseConfig="false" visible="false" />
										</row:child>
									</row:VerticalLayoutContainer>
								</gxt:ContentPanel>

                                <gxt:ContentPanel ui:field="questionsPanel" appearance="{accordionLayoutAppearance}" borders="false"
                                    headingText="Вопросы" animCollapse="false">
                                    <row:VerticalLayoutContainer>
                                        <row:child layoutData="{maxWidthMinHeightVLData}">
                                            <toolbar:ToolBar visible="true">
                                                <button:TextButton ui:field="addQuestionButton"
                                                    icon="{icons.next16x16}" iconAlign="RIGHT" text="Добавить в тест" />
                                                <toolbar:FillToolItem />
                                                <att:SearchField ui:field="questionsSearchField"
                                                    validationDelay="700" emptyText="Поиск" />
                                            </toolbar:ToolBar>
                                        </row:child>
                                        <row:child layoutData="{maxWidthMaxHeightVLData}">
                                            <grid:Grid ui:field="questionsGrid" />
                                        </row:child>
                                        <row:child layoutData="{maxWidthMinHeightVLData}">
                                            <toolbar:PagingToolBar ui:field="questionsPager"
                                                pageSize="50" reuseConfig="false" />
                                        </row:child>
                                    </row:VerticalLayoutContainer>
                                </gxt:ContentPanel>

							</row:AccordionLayoutContainer>
						</gxt:ContentPanel>
					</row:west>
					<row:center layoutData="{centerData}">
						<gxt:ContentPanel appearance="{contentPanelAppearance}"
							headingText="Содержимое теста">
							<row:VerticalLayoutContainer>
								<row:child layoutData="{maxWidthMinHeightVLData}">
									<toolbar:ToolBar visible="true">
										<toolbar:FillToolItem />
										<button:TextButton ui:field="removeEntryButton"
											icon="{icons.prev16x16}" iconAlign="LEFT" text="Убрать из теста" />
									</toolbar:ToolBar>
								</row:child>
								<row:child layoutData="{maxWidthMaxHeightVLData}">
									<grid:Grid ui:field="entriesGrid" />
								</row:child>
							</row:VerticalLayoutContainer>
						</gxt:ContentPanel>
					</row:center>
					<row:south layoutData="{southData}">
						<gxt:ContentPanel appearance="{contentPanelAppearance}" headingText="Публикации">
                            <row:VerticalLayoutContainer>
<!--                                 <row:child layoutData="{maxWidthMinHeightVLData}"> -->
<!--                                     <toolbar:ToolBar visible="true"> -->
<!--                                         <toolbar:FillToolItem /> -->
<!--                                         <button:TextButton ui:field="removeEntry" -->
<!--                                             icon="{icons.prev16x16}" iconAlign="LEFT" text="Убрать из теста" /> -->
<!--                                     </toolbar:ToolBar> -->
<!--                                 </row:child> -->
                                <row:child layoutData="{maxWidthMaxHeightVLData}">
                                    <grid:Grid ui:field="publicationsGrid" />
                                </row:child>
                            </row:VerticalLayoutContainer>
						</gxt:ContentPanel>
					</row:south>
				</row:BorderLayoutContainer>
			</row:child>
		</row:VerticalLayoutContainer>
		<gxt:button>
			<button:TextButton ui:field="saveButton" text="Сохранить" />
		</gxt:button>

		<gxt:button>
			<button:TextButton ui:field="cancelButton" text="Отмена" />
		</gxt:button>

	</gxt:Window>

</ui:UiBinder>